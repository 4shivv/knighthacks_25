# Gemini API Configuration (for ADK)
GEMINI_API_KEY=your_api_key_here
# ADK Agent Model (use flash-exp for text, flash-live-001 for audio streaming)
GEMINI_MODEL=gemini-2.0-flash-exp
# Set to TRUE to use Vertex AI instead of Google AI Studio
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# YOLO Configuration (YOLO11 - latest version)
# Options: yolo11n.pt (nano, 1.5ms, recommended for real-time)
#          yolo11s.pt (small, 2.5ms, better accuracy)
#          yolo11m.pt (medium, 4.7ms)
#          yolo11l.pt (large, 6.2ms)
#          yolo11x.pt (xlarge, 11.3ms, best accuracy)
YOLO_MODEL_PATH=yolo11n.pt
CONFIDENCE_THRESHOLD=0.7

# Sensor Configuration
SENSOR_TIMEOUT_MS=2000
LIDAR_WS_PORT=8001
CAMERA_WS_PORT=8002

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
DEBUG=True

# Loop Intervals (milliseconds)
SENSOR_LOOP_INTERVAL=500
FUSION_LOOP_INTERVAL=500
DETECTION_LOOP_INTERVAL=3000
PLANNING_LOOP_INTERVAL=1000
OUTPUT_LOOP_INTERVAL=3000

# Mission Configuration
DEFAULT_OBJECTIVE=Find a red coffee mug
SUCCESS_DISTANCE_THRESHOLD=0.5
SUCCESS_CONFIDENCE_THRESHOLD=0.95
